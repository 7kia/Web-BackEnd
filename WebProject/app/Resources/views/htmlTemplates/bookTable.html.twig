<div class="bookTable">
    <table class="treetable">
        <tbody>
            <tr>
            {% for book in bookList %}
                <th>
                    <img src="{{ book.cover }}" alt={{ book.name }}
                         width="100" height="150">
                    <a>{{ book.name }}</a>
                    <div class="star">
                    </div>
                    <script type="text/javascript">
                        $('.star').raty();
                    </script>
                </th>
            {% endfor %}
            </tr>
        </tbody>
    </table>
</div>